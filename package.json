{
  "name": "css-typed-om",
  "version": "0.4.0",
  "description": "Use CSS Typed Object Model in the browser",
  "author": "Jonathan Neal <jonathantneal@hotmail.com>",
  "license": "CC0-1.0",
  "repository": "csstools/css-typed-om",
  "homepage": "https://github.com/csstools/css-typed-om#readme",
  "bugs": "https://github.com/csstools/css-typed-om/issues",
  "type": "module",
  "main": "index.umd.js",
  "module": "index.es.js",
  "files": [
    "index.umd.js",
    "index.es.js"
  ],
  "scripts": {
    "prepublishOnly": "npm test",
    "pretest": "rollup -c .rollup.js --silent",
    "test": "echo 'Running tests...'; npm run test:js && node --test",
    "test:js": "eslint *.js lib/*.js --cache --ignore-path .gitignore --quiet"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.20",
    "@babel/eslint-parser": "7.22.15",
    "@babel/preset-env": "7.22.20",
    "@rollup/plugin-babel": "6.0.3",
    "babel-plugin-array-includes": "2.0.3",
    "eslint": "8.50.0",
    "eslint-config-dev": "3.3.1",
    "pre-commit": "^1.2.2",
    "rollup": "3.29.2"
  },
  "eslintConfig": {
    "extends": "dev",
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false
    }
  },
  "keywords": [
    "css",
    "number",
    "percent",
    "em",
    "ex",
    "ch",
    "rem",
    "vw",
    "vh",
    "vmin",
    "vmax",
    "cm",
    "mm",
    "in",
    "pt",
    "pc",
    "px",
    "Q",
    "deg",
    "grad",
    "rad",
    "turn",
    "s",
    "ms",
    "Hz",
    "kHz",
    "dpi",
    "dpcm",
    "dppx",
    "fr",
    "StylePropertyMap",
    "CSSUnitValue",
    "CSSStyleValue",
    "CSSKeywordValue",
    "styleMap",
    "attributeStyleMap",
    "computedStyleMap"
  ]
}
